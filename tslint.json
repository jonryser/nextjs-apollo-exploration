{
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "rules": {
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "member-access": [
            true,
            "no-public"
        ],
        "ordered-imports": false,
        "object-literal-key-quotes": [
            true,
            "as-needed"
        ],
        "interface-name": false,
        "jsx-no-multiline-js": false,
        "no-empty-interface": false,
        "jsx-no-lambda": true
    }
}